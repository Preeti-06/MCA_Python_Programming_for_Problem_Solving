   "source": [
    "import string\n",
    "\n",
    "text = input(\"Enter text: \").lower()\n",
    "\n",
    "for p in string.punctuation:\n",
    "    text = text.replace(p, \"\")\n",
    "\n",
    "words = text.split()\n",
    "freq = {}\n",
    "\n",
    "for w in words:\n",
    "    freq[w] = freq.get(w, 0) + 1\n",
    "\n",
    "print(freq)"
