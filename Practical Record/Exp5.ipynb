   "source": [
    "s = input(\"Enter sentence: \")\n",
    "vowels = \"aeiouAEIOU\"\n",
    "\n",
    "s = list(s)\n",
    "i, j = 0, len(s)-1\n",
    "\n",
    "while i < j:\n",
    "    if s[i] not in vowels:\n",
    "        i += 1\n",
    "    elif s[j] not in vowels:\n",
    "        j -= 1\n",
    "    else:\n",
    "        s[i], s[j] = s[j], s[i]\n",
    "        i += 1\n",
    "        j -= 1\n",
    "\n",
    "print(\"\".join(s))"
   ]
